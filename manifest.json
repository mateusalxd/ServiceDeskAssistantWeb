{
   "manifest_version":2,
   "author":"Mateus Alexandre Gonçalves da Silva",
   "name":"Service Desk Assistant - Web",
   "description":"Assistente para Service Desk versão Web",
   "version":"1.4",
   "content_scripts":[
      {
         "matches":[
            "*://multi-prd/*",
            "*://servicedesk.cooxupe.com.br/*",
            "file:///*"
         ],
         "js":[
            "principal.js"
         ]
      }
   ],
   "web_accessible_resources":[
      "compartilhado.js"
   ],
   "permissions":[
      "activeTab",
      "*://multi-prd/*",
      "*://servicedesk.cooxupe.com.br/*",      
      "file:///*"
   ]
}
